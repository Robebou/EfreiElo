<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title></title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="css/index.css" type="text/css">
    </head>
    <body>
        <header class="main_header">
            <span class="container_header">
                <span class="logo"><img id="logo_image" src="https://github.com/Robebou/EfreiElo/blob/main/ressource/image/Logo_Blue.png?raw=true" alt="4eSLogo"/></span>
                <nav class="main-nav">
                    <ul class="main-nav-list">
                        <li>
                            <a href="#">Classement par Poste</a>
                        </li>
                        <li>
                            <a href="#">Graphique</a>
                        </li>
                        <li>
                            <a href="#">Se Connecter</a>
                        </li>
                        <li>
                            <a href="#">Pari eSportif</a>
                        </li>
                    </ul>
                </nav>
            </span>
        </header>
            <div id="Container_Tab"> 
                <table class = "Tab_Joueur">
                    <thead>
                        <tr>
                            <th> Icone </th>
                            <th> Nom d'invocateur </th>
                            <th> Level </th>
                            <th> Classement </th>
                            <th> Winrate </th>
                        </tr>
                    </thead>
                    <tfoot>
                        <tr>
                            <th><img src="https://static.wikia.nocookie.net/leagueoflegends/images/5/5f/Season_2019_-_Challenger_1.png"/></th>
                            <th><img src="https://static.wikia.nocookie.net/leagueoflegends/images/7/76/Season_2019_-_Grandmaster_1.png"/></th>
                            <th><img src="https://static.wikia.nocookie.net/leagueoflegends/images/1/11/Season_2019_-_Master_1.png"/></th>
                            <th><img src="https://static.wikia.nocookie.net/leagueoflegends/images/9/91/Season_2019_-_Diamond_1.png"/></th>
                        </tr>
                    </tfoot>
                    <tbody>
                    <% info_players.forEach(info_players => { %>
                        <tr>
                            <td> <img id="icon_summoner" src ="<%= info_players.icone%>"> </td>
                            <td> <%= info_players.nom_invocateur%></td>
                            <td> <%= info_players.lvl%> </td>
                            <td> <%= info_players.elo%> </td>
                        </tr>
                    <% }); %>   
                    </tbody>
                </table>
            </div>
        </h2>
        <footer>

        </footer>
        <script src="" async defer></script>
    </body>
</html>